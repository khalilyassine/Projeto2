package modelo;

public class Lamp implements Drain {
	
	private InputPin pin;	
	

	public Lamp() {
		pin = new InputPin(0);
	}

	public InputPin getInputPin(int index) {
        return pin;
	}
	
	public boolean getOutput(InputPin pin){
		return this.pin.getSource().getOutputValue(this.pin.getIndex());
	}
}
